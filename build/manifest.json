{
  "name": "Clickable Google Meet logo",
  "version": "1.1",
  "description": "Make the Google Meet logo clickable",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://meet.google.com/_meet/*", "https://meet.google.com/*"],
      "js": ["contentScript.js"],
      "run_at": "document_idle"
    }
  ]
}
